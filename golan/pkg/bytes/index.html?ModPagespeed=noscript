import (
	&#34;bytes&#34;
	&#34;fmt&#34;
)

func main() {
	var b = []byte(&#34;Goodbye,, world!&#34;)
	b = bytes.TrimPrefix(b, []byte(&#34;Goodbye,&#34;))
	b = bytes.TrimPrefix(b, []byte(&#34;See ya,&#34;))
	fmt.Printf(&#34;Hello%s&#34;, b)
}
</textarea></div>
