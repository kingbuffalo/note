import (
	&#34;bytes&#34;
	&#34;os&#34;
)

func main() {
	var b = []byte(&#34;Hello, goodbye, etc!&#34;)
	b = bytes.TrimSuffix(b, []byte(&#34;goodbye, etc!&#34;))
	b = bytes.TrimSuffix(b, []byte(&#34;gopher&#34;))
	b = append(b, bytes.TrimSuffix([]byte(&#34;world!&#34;), []byte(&#34;x!&#34;))...)
	os.Stdout.Write(b)
}
</textarea></div>
